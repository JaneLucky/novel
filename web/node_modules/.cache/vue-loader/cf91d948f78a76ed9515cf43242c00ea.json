r\n                this.desc=false;\r\n            }\r\n        },\r\n        loadList(){\r\n            this.axios.get(\"http://127.0.0.1:3000/book/details\",{\r\n                params:{\r\n                    bid:this.bid\r\n                }\r\n            }).then((result)=>{\r\n                this.contentList=result.data.msg;\r\n                console.log(this.contentList);\r\n                if(result.data.msg.length==0){\r\n                    this.hiddenList.display=\"block\";\r\n                }\r\n            })\r\n        }\r\n    },\r\n    created(){\r\n        this.bid=this.$route.query.bid;\r\n        this.loadList();\r\n    },\r\n    components:{\r\n        Header\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n.noList{\r\n    text-align: center;            \r\n    color:\"#666\";\r\n    height: 5rem;\r\n    line-height: 10rem;\r\n}\r\nbody,ul,li{\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\nul>li{\r\n    list-style: none;\r\n}\r\nul>li a{\r\n    color: #888;\r\n}\r\n.totalCount{\r\n    border-bottom: 1px solid #eee;\r\n    border-top: 1px solid #eee;\r\n    padding: 0.6rem 0.3rem;\r\n    color: #666;\r\n}\r\n.totalCount div:first-child{\r\n    font-size: 1.2rem;\r\n    float: left;\r\n    line-height: 2rem;\r\n}\r\n.totalCount div:last-child{\r\n    width: 9%;\r\n    float: right;\r\n}\r\n.totalCount div img{\r\n    display: block;\r\n    width: 100%;\r\n}\r\n\r\n\r\n\r\n\r\nul>li{\r\n    padding: 1rem 1rem;\r\n    border-bottom: 0.5px solid #eee;\r\n    color: #666;\r\n}\r\nul>li a{\r\n    width: 100%;\r\n    display: block;\r\n}\r\n.clearFixed::after{\r\n    content: \"\";\r\n    display: block;\r\n    clear: both;\r\n    }\r\n\r\n</style>\r\n"]}]}